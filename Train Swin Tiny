    "#             )\n",
    "#             val_loss, val_acc = epoch_run(\n",
    "#                 ema.ema_module, val_ld,\n",
    "#                 optimizer, scaler, scheduler,\n",
    "#                 mixup_fn=None, ema=None,\n",
    "#                 acc=accum_steps, train=False\n",
    "#             )\n",
    "\n",
    "#             print(\n",
